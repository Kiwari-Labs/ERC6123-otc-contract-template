{
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "bracketSpacing": false,
    "plugins": [
        "prettier-plugin-solidity"
    ],
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "parser": "slang",
                "printWidth": 140,
                "tabWidth": 4,
                "useTabs": false,
                "singleQuote": false,
                "bracketSpacing": false
            }
        }
    ]
}